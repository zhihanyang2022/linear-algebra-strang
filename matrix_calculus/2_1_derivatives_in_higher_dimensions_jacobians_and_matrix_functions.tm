  Difference:

  <\eqnarray*>
    <tformat|<table|<row|<cell|>|<cell|>|<cell|f<around*|(|<with|font-series|bold|x>+d<with|font-series|bold|x>|)>-f<around*|(|<with|font-series|bold|x>|)>>>|<row|<cell|>|<cell|=>|<cell|<with|font-series|bold|><around*|(|<with|font-series|bold|x>+d<with|font-series|bold|x>|)><around*|(|<with|font-series|bold|x>+d<with|font-series|bold|x>|)><rsup|T><around*|(|<with|font-series|bold|x>+d<with|font-series|bold|x>|)>-<with|font-series|bold|x><with|font-series|bold|x><rsup|T><with|font-series|bold|x>>>|<row|<cell|>|<cell|=>|<cell|<around*|(|<with|font-series|bold|x>+d<with|font-series|bold|x>|)><around*|(|<with|font-series|bold|x><rsup|T>+<around*|(|d<with|font-series|bold|x>|)><rsup|T>|)><around*|(|<with|font-series|bold|x>+d<with|font-series|bold|x>|)>-<with|font-series|bold|x><with|font-series|bold|x><rsup|T><with|font-series|bold|x>>>|<row|<cell|>|<cell|=>|<cell|<around*|(|<with|font-series|bold|x><with|font-series|bold|x><rsup|T>+<with|font-series|bold|x><around*|(|d<with|font-series|bold|x>|)><rsup|T>+<around*|(|d<with|font-series|bold|x>|)><with|font-series|bold|x><rsup|T>+<around*|(|d<with|font-series|bold|x>|)><around*|(|d<with|font-series|bold|x>|)><rsup|T>|)><around*|(|<with|font-series|bold|x>+d<with|font-series|bold|x>|)>-<with|font-series|bold|x><with|font-series|bold|x><rsup|T><with|font-series|bold|x>>>|<row|<cell|>|<cell|=>|<cell|<with|font-series|bold|x><around*|(|d<with|font-series|bold|x>|)><rsup|T><with|font-series|bold|x>+<around*|(|d<with|font-series|bold|x>|)><with|font-series|bold|x><rsup|T><with|font-series|bold|x>+<around*|(|d<with|font-series|bold|x>|)><around*|(|d<with|font-series|bold|x>|)><rsup|T><with|font-series|bold|x>+<with|font-series|bold|x><with|font-series|bold|x><rsup|T><around*|(|d<with|font-series|bold|x>|)>+<with|font-series|bold|x><around*|(|d<with|font-series|bold|x>|)><rsup|T><around*|(|d<with|font-series|bold|x>|)>+<around*|(|d<with|font-series|bold|x>|)><with|font-series|bold|x><rsup|T><around*|(|d<with|font-series|bold|x>|)>+<around*|(|d<with|font-series|bold|x>|)><around*|(|d<with|font-series|bold|x>|)><rsup|T><around*|(|d<with|font-series|bold|x>|)>>>>>
