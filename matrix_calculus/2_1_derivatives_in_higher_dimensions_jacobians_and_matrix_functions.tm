    <tformat|<table|<row|<cell|f<around*|(|<with|font-series|bold|x>+d<with|font-series|bold|x>|)>-f<around*|(|<with|font-series|bold|x>|)>>|<cell|=>|<cell|<around*|(|<with|font-series|bold|x>+d<with|font-series|bold|x>|)>\<odot\><around*|(|<with|font-series|bold|x>+d<with|font-series|bold|x>|)>-<with|font-series|bold|x>\<odot\><with|font-series|bold|x>>>|<row|<cell|>|<cell|=>|<cell|<with|font-series|bold|x>\<odot\><with|font-series|bold|x>+2<with|font-series|bold|x>\<odot\><around*|(|d<with|font-series|bold|x>|)>+<around*|(|d<with|font-series|bold|x>|)>\<odot\><around*|(|d<with|font-series|bold|x>|)>-<with|font-series|bold|x>\<odot\><with|font-series|bold|x>>>|<row|<cell|>|<cell|=>|<cell|2<with|font-series|bold|x>\<odot\><around*|(|d<with|font-series|bold|x>|)>+<around*|(|d<with|font-series|bold|x>|)>\<odot\><around*|(|d<with|font-series|bold|x>|)>>>|<row|<cell|>|<cell|=>|<cell|diag<around*|(|2<with|font-series|bold|x>|)><around*|(|d<with|font-series|bold|x>|)>+<around*|(|d<with|font-series|bold|x>|)>\<odot\><around*|(|d<with|font-series|bold|x>|)>>>>>
